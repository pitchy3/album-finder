{
  "name": "album-finder-server",
  "version": "2.0.0",
  "description": "AlbumFinder server with modular architecture",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
