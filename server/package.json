    "express-session": "^1.18.2",
    "node-cache": "^5.1.2",
    "openid-client": "^5.6.4",
    "redis": "^4.6.12",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "nock": "^13.5.6",
    "nodemon": "^3.0.2",
    "redis-mock": "^0.56.3",
    "supertest": "^6.3.3"
